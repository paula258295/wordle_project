{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/(auth)/signin/page.js", "src/app/(auth)/signup/page.js", "src/app/components/forms/SignupForm.js", "src/app/components/forms/SigninForm.js", "src/app/account/page.js", "src/app/account/layout.js", "src/app/edit-profile/page.js", "src/app/edit-profile/layout.tjs", "src/app/edit-profile/EditAccount.js"  ],
  "exclude": [
    "node_modules"
  ]
}
